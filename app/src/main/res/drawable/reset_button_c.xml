<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">
            <shape>
                <gradient
                    android:angle="270"
                    android:startColor="#1063FF"
                    android:endColor="#1212B0">
                </gradient>
                <corners
                    android:radius="7dp">
                </corners>
                <stroke
                    android:color="#FFFFFF"
                    android:width=".2dp">
                </stroke>
            </shape>
    </item>

    <item android:state_pressed="true">
            <shape>
                <gradient
                    android:angle="270"
                    android:startColor="#2323A7"
                    android:endColor="#4786FC">
                </gradient>
                <corners
                    android:radius="7dp">
                </corners>
                <stroke
                    android:color="#FFFFFF"
                    android:width=".2dp">
                </stroke>
            </shape>
    </item>

</selector>